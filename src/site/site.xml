<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="WFS-KML Gateway">
  <skin>
    <groupId>es.uva.idelab</groupId>
    <artifactId>idelab-skin</artifactId>
    <version>1.1-SNAPSHOT</version>
  </skin>
  <publishDate position="left" format="dd MMM yyyy"/>
  <version position="left"/>
  <bannerLeft>
    <name>IDELab</name>
    <src>http://img.idelab.uva.es/world_logo.png</src>
    <href>http://idelab.uva.es/</href>
  </bannerLeft>
  <bannerRight>
    <name>${project.name}</name>
    <href>http://mvn.idelab.uva.es/${project.artifactId}</href>
  </bannerRight>
  <body>
    <!-- ==================================================== -->
    <!--     Links to appears just below the right banner     -->
    <!-- ==================================================== -->
    <links>
      <item name="IDELab" href="http://www.idelab.uva.es"/>
    </links>

    <!-- ==================================================== -->
    <!--     Menus to appears on the left side of each page   -->
    <!-- ==================================================== -->
    <menu name="${project.name}">
      <item name="Introduction" href="index.html"/>
      <item name="Features" href="features.html"/>
      <item name="Glossary" href="glossary.html"/>
      <item name="Examples" href="examples.html"/>
      <item name="Screenshots" href="screenshots.html"/>
      <item name="Download" href="http://rep.idelab.uva.es"/>    
      <item name="Web site" href="http://dev.idelab.uva.es/wfs2kml"/>    
    </menu>
    
    <menu ref="parent" />
    <menu ref="modules" />
    <menu ref="reports" collapse="true" />

  </body>  
  
</project>